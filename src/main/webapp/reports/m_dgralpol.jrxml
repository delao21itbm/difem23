	<queryString>
		<![CDATA[SELECT
       PL.CLVPOL,
       PL.TIPPOL TIPO,
	   PL.CONPOL NUM,
       PL.STAPOL STATUS,
       PL.STAAFE AFECTADO,
     TO_CHAR(PL.FECPOL,'DD/MM/YYYY') FECHA,
       PL.CAPPOL CAPTURO,
       PD.RENPOL RENGLON,
       PD.CUENTA CTA,
       PD.SCTA,
       PD.SSCTA,
       PD.SSSCTA,
       PD.SSSSCTA,
      TRIM(TO_CHAR( PD.REFPOL ,'999999999999990'))|| ' '||PD.DN  REFPOL,
       PD.CONCEP,
       PD.CANPOL,
       PD.CANPOLH,
       PD.TIPPOL||PD.CONPOL GRUPO
     FROM POLIEN PL
  INNER JOIN POLIDE PD ON PD.TIPPOL = PL.TIPPOL 
                         AND PD.MESPOL = PL.MESPOL
                         AND PD.CONPOL = PL.CONPOL
                         AND PD.IDSECTOR = PL.IDSECTOR
                         AND PD.ANOPOL = PL.ANOPOL
   WHERE PL.IDSECTOR= $P{p_IdSector}
         AND PL.MESPOL= $P{p_Mes}
           AND PL.ANOPOL = $P{p_An}
   ORDER BY PL.TIPPOL,
            PL.CONPOL,
            PD.RENPOL]]>
	</queryString>
