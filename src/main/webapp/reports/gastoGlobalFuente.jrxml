	<queryString>
		<![CDATA[SELECT D.SSSCTA,
	SUM(D.CANPOL) CARGO,
	SUM(D.CANPOLH) ABONO ,
	SUM(D.CANPOL)-SUM(D.CANPOLH) DIF
FROM POLIDE D
WHERE D.MESPOL = $P{mes} AND
	D.IDSECTOR = $P{sector} AND 
	D.CUENTA = $P{cuenta} AND 
	D.SSSCTA LIKE  $P{ssscta} ||  '%' AND
	D.SSCTA <>'' AND 
	SUBSTR(D.SSCTA ,13,3) LIKE $P{sscta}||'%'
GROUP BY
	D.SSSCTA
]]>
	</queryString>
